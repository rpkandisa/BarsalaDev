                $('#btTotalPriceRef').html("$" + discountedPrice);
                $('#pcTotalPriceRef').html("$" + discountedPrice);
                $('#discount').val(discountPercentage);
                $('#pcdis').html(discountPercentage+ "%");
                //Recalculating TaxAmount
