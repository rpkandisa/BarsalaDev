                //     tempunitPrice -= gb_oldbooking.unusedcredit;
                // }

                //Calculating discount amount from final price
                var discountAmount = 
                        (
                            (
                                parseFloat(tempunitPrice) + parseFloat(unitCleanfee) - parseFloat(bttotalprice) + 
                                (
                                    (( parseFloat(tempunitPrice) + parseFloat(unitCleanfee)) *parseFloat(unitTax))
                                )
                            )/(1+parseFloat(unitTax))
                        ).toFixed(2);
                var discountedPrice = (tempunitPrice - discountAmount).toFixed(2);
