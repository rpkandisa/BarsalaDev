            function calculateAmountsWhenFinalPriceChanges(){
                //debugger;
                let unitcleaningfees = parseFloat(unitCleanfee);
                if($('#chkWaiverCleaning').is(':checked') == true){
                    unitcleaningfees = 0;
                }

                var bttotalprice = $('#bttotalprice').val();
                if(!bttotalprice || isNaN(bttotalprice))
                    bttotalprice = 0;
                bttotalprice = parseFloat(bttotalprice).toFixed(2);
                
                $('#pctotalprice').html("$" + bttotalprice);
                var tempunitPrice = unitPrice;
                var unusedcredit = gb_oldbooking.unusedcredit;

                var NPAndUC = parseFloat(bttotalprice) + parseFloat(unusedcredit);
                var CLFAndCLFTX = parseFloat(unitcleaningfees) + (parseFloat(unitcleaningfees) * parseFloat(unitTax));
                var subamtbftax = NPAndUC - CLFAndCLFTX;
                var subamt = subamtbftax / (1 + parseFloat(unitTax));
                var disamt = parseFloat(unitPrice) - subamt;
                var dispercent = (100 * disamt) / parseFloat(unitPrice);
                var taxamt = (parseFloat(unitcleaningfees) + subamt) * parseFloat(unitTax);

                $('#btTotalPriceRef').html("$" + subamt.toFixed(2));
                $('#pcTotalPriceRef').html("$" + subamt.toFixed(2));
                $('#discount').val(dispercent.toFixed(2));
                $('#pcdis').html(dispercent.toFixed(2)+ "%");
                $('#bttax').html("$" + taxamt.toFixed(2));
                $('#pctax').html("$" + taxamt.toFixed(2));
            };

