                var setObj = {}, val = {};
                this.count = 0;
                this.add = function (str) {
                    setObj[str] = val;
                    return ++this.count
                };
            
                this.contains = function (str) {
                    return setObj[str] === val;
                };
            
                this.remove = function (str) {
                    delete setObj[str];
                    return --this.count
                };
            
                this.values = function () {
                    var values = [];
                    for (var i in setObj) {
                        if (setObj[i] === val) {
